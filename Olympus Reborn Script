local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Olympus Reborn (By: Sorln#3037)", "Midnight")
local Tab = Window:NewTab("Powers")
local Section = Tab:NewSection("Gods and Goddesses Powers")
Section:NewButton("Rainbow Wave", "Iris Rainbow Wave", function()
    print("Clicked")
    local ohString1 = "wave"
local ohVector32 = Vector3.new(-60.55952072143555, 255.11248779296875, 68.51655578613281)

game:GetService("ReplicatedStorage").Olympians.Goddesses.Iris.PowerEvents.IrisEvents:FireServer(ohString1, ohVector32)
end)
